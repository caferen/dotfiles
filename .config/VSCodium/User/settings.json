{
    "editor.formatOnSave": true,
    "python.languageServer": "Pylance",
    "editor.cursorStyle": "block",
    "rust-analyzer.debug.openDebugPane": true,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "terminal.integrated.gpuAcceleration": "on",
    "workbench.colorCustomizations": {
        "editorInlayHint.foreground": "#696969",
        "editorInlayHint.background": "#0000",
        "editorInlayHint.typeBackground": "#0000",
    },
    "editor.lineNumbers": "relative",
    "editor.mouseWheelScrollSensitivity": 0.5,
    "git.enableStatusBarSync": false,
    "editor.tabCompletion": "on",
    "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.inlayHints.maxLength": 15,
    "git.confirmSync": false,
    "githubPullRequests.pullBranch": "never",
    "githubPullRequests.fileListLayout": "flat",
    "explorer.confirmDragAndDrop": false,
    "editor.stickyScroll.enabled": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "telemetry.telemetryLevel": "off",
    "gitlens.telemetry.enabled": false,
    "window.restoreWindows": "none",
    "editor.minimap.enabled": true,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.hideOnStartup": "whenEmpty",
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "editor.renderLineHighlight": "none",
    "workbench.activityBar.location": "top",
    "window.titleBarStyle": "custom",
    "editor.overviewRulerBorder": false,
    "workbench.sideBar.location": "right",
    "git.openRepositoryInParentFolders": "never",
    "diffEditor.renderSideBySide": false,
    "workbench.colorTheme": "GitHub Dark Default",
    "editor.fontFamily": "MesloLGS Nerd Font",
    "window.zoomLevel": 1,
    "diffEditor.maxComputationTime": 0,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.showTabs": "single",
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "update.showReleaseNotes": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "twinny.fileContextEnabled": true,
    "twinny.completionCacheEnabled": true,
    "twinny.ollamaHostname": "localhost",
    "window.customTitleBarVisibility": "auto",
}